<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/txt_john"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="John McDonald"
        android:textSize="30dp"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="15dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/txt_los"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Los Angles,CA"
        android:textSize="20dp"
        android:layout_marginLeft="55dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/txt_john"

        />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="25dp"
        android:background="@drawable/ic_place_black_24dp"
        app:layout_constraintTop_toBottomOf="@id/txt_john"
        app:layout_constraintRight_toLeftOf="@id/txt_los"
        android:layout_marginRight="3dp"
        />

    <ImageView
        android:layout_width="110dp"
        android:layout_height="150dp"
        android:background="@drawable/john"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@id/txt_john"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="32dp"
        android:layout_marginLeft="1dp"
        app:layout_constraintHorizontal_bias="0.6"
        />

    <Button
        android:id="@+id/btn_follow"
        android:layout_width="110dp"
        android:layout_height="30dp"
        android:text="Follow"
        android:textAllCaps="false"
        app:layout_constraintTop_toBottomOf="@id/txt_los"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textSize="16dp"
        android:textColor="#E2DBDB"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="35dp"
        android:background="@drawable/rounded_buttom"

        />
    <TextView
        android:id="@+id/txt_34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="34"
        android:textStyle="bold"
        android:textSize="16dp"
        app:layout_constraintTop_toBottomOf="@id/btn_follow"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="23dp"
        />
    <TextView
        android:id="@+id/txt_1256"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1256"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintLeft_toRightOf="@id/txt_34"
       app:layout_constraintTop_toBottomOf="@id/btn_follow"
        app:layout_constraintBaseline_toBaselineOf="@id/txt_34"
        android:layout_marginLeft="50dp"
        />
    <TextView
        android:id="@+id/txt_post"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Posts"
        android:textSize="14dp"
        app:layout_constraintTop_toBottomOf="@id/txt_34"
       app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="50dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Followers"
        android:textSize="14dp"
        app:layout_constraintTop_toBottomOf="@id/txt_1256"
        app:layout_constraintLeft_toRightOf="@id/txt_post"
        app:layout_constraintBaseline_toBaselineOf="@id/txt_post"
        android:layout_marginLeft="35dp"

        />

    <ImageView
        android:id="@+id/img_a"
        android:layout_width="72dp"
        android:layout_height="80dp"
        android:layout_marginLeft="26dp"
        android:layout_marginTop="28dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/a"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/img_b"
        app:layout_constraintTop_toBottomOf="@+id/txt_post"

        />

    <ImageView
        android:id="@+id/img_b"
        android:layout_width="72dp"
        android:layout_height="80dp"
        android:background="@drawable/b"
        app:layout_constraintCircle="@id/img_a"
        app:layout_constraintCircleRadius="72dp"
        app:layout_constraintCircleAngle="90"
        app:layout_constraintBaseline_toBaselineOf="@id/img_a"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/img_c"
        android:layout_width="72dp"
        android:layout_height="80dp"
        android:background="@drawable/c"
        app:layout_constraintCircle="@id/img_b"
        app:layout_constraintCircleAngle="90"
        app:layout_constraintCircleRadius="72dp"
        app:layout_constraintBaseline_toBaselineOf="@id/img_a"
        />
    <ImageView
        android:id="@+id/img_d"
        android:layout_width="72dp"
        android:layout_height="80dp"
        android:background="@drawable/d"
        app:layout_constraintCircle="@id/img_c"
        app:layout_constraintCircleAngle="90"
        app:layout_constraintCircleRadius="72dp"
        app:layout_constraintBaseline_toBaselineOf="@id/img_a"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/img_e"
        android:layout_width="72dp"
        android:layout_height="80dp"
        android:background="@drawable/e"
        app:layout_constraintCircle="@id/img_d"
        app:layout_constraintCircleRadius="72dp"
        app:layout_constraintCircleAngle="90"
        app:layout_constraintBaseline_toBaselineOf="@id/img_a"
        tools:ignore="MissingConstraints" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+34"
        android:textSize="13dp"
        android:textColor="#D0D3D5"
        app:layout_constraintLeft_toRightOf="@id/img_e"
        app:layout_constraintRight_toLeftOf="@id/img_e"
        app:layout_constraintTop_toTopOf="@id/img_e"

        android:layout_marginTop="15dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="photos"
        android:textSize="10dp"
        android:textColor="#D0D3D5"
        app:layout_constraintLeft_toRightOf="@id/img_e"
        app:layout_constraintRight_toLeftOf="@id/img_e"
        app:layout_constraintTop_toTopOf="@id/img_e"
        android:layout_marginTop="35dp"
        />
    <ImageView
        android:id="@+id/f"
        android:layout_width="19dp"
        android:layout_height="23dp"
        android:background="@drawable/ic_facbook"
        app:layout_constraintTop_toBottomOf="@id/img_a"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="20dp"
        />
    <ImageView
        android:id="@+id/t"
        android:layout_width="19dp"
        android:layout_height="23dp"
        android:background="@drawable/ic_twitter"
        app:layout_constraintCircle="@id/f"
        app:layout_constraintCircleRadius="60dp"
        app:layout_constraintCircleAngle="90"
        app:layout_constraintBaseline_toBaselineOf="@id/f"
        tools:ignore="MissingConstraints"
        />
    <ImageView
        android:id="@+id/b"
        android:layout_width="19dp"
        android:layout_height="23dp"
        android:background="@drawable/ic_be"
        app:layout_constraintCircle="@id/t"
        app:layout_constraintCircleRadius="60dp"
        app:layout_constraintCircleAngle="90"
        app:layout_constraintBaseline_toBaselineOf="@id/f"
        tools:ignore="MissingConstraints"
        />
    <ImageView
        android:id="@+id/v"
        android:layout_width="19dp"
        android:layout_height="23dp"
        android:background="@drawable/ic_viber"
        app:layout_constraintCircle="@id/b"
        app:layout_constraintCircleRadius="60dp"
        app:layout_constraintCircleAngle="90"
        app:layout_constraintBaseline_toBaselineOf="@id/f"
        tools:ignore="MissingConstraints"
        />
    <ImageView
        android:layout_width="19dp"
        android:layout_height="23dp"
        android:background="@drawable/ic_wechat"
        app:layout_constraintCircle="@id/v"
        app:layout_constraintCircleRadius="60dp"
        app:layout_constraintCircleAngle="90"
        app:layout_constraintBaseline_toBaselineOf="@id/t"
        tools:ignore="MissingConstraints"
        />
    <View
        android:id="@+id/line"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#8AA4B1C9"
        app:layout_constraintTop_toBottomOf="@id/f"
        android:layout_marginTop="40dp"
        />
    <TextView
        android:id="@+id/about"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="About"
        android:textSize="24dp"
        android:textColor="#000000"
        app:layout_constraintTop_toBottomOf="@id/line"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        />
    <TextView
        android:layout_width="360dp"
        android:layout_height="93dp"
        android:text="Him poetic eye.Searched any remedies. Trial.Was poverty a schemas him he dressing mountains and as surprise haven't subject and could a entered would in the of be not posts,doing the never publication legs."
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@id/about"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        />





</androidx.constraintlayout.widget.ConstraintLayout>